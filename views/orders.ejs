<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Order Details</title>
    <link rel="stylesheet" href="/styles.css"> <!-- Optional if you have CSS -->
</head>
<body>
    <h1>Order Details</h1>

    <table border="1">
        <thead>
            <tr>
                <th>Emp Id</th>
                <th>Order Date</th>
                <th>Client Id</th>
                <th>Product Id</th>
                <th>Quantity</th>
                <th>Order Value</th>
                <th>Order Id</th>
            </tr>
        </thead>
        <tbody>
            <% orders.forEach(order => { %>
                <tr>
                    <td><%= order.EmpId %></td>
                    <td><%= order.OrderDate %></td>
                    <td><%= order.ClientId %></td>
                    <td><%= order.ProductId %></td>
                    <td><%= order.Quantity %></td>
                    <td><%= order.OrderValue %></td>
                    <td><%= order.OrderId %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
